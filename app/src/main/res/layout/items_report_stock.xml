<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:layout_marginTop="12dp"    android:layout_marginBottom="12dp">    <TextView        android:id="@+id/tv_report_stock_title"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="16dp"        android:fontFamily="@font/noto_sans_cjk_kr_regular"        android:includeFontPadding="false"        android:letterSpacing="-0.07"        android:text="글로벌 인터넷 플랫폼으로 도약, 적극 매수"        android:textColor="#000000"        android:textSize="16sp"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <TextView        android:id="@+id/tv_report_stock_name"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="16dp"        android:fontFamily="@font/noto_sans_cjk_kr_regular"        android:includeFontPadding="false"        android:letterSpacing="-0.07"        android:text="하나투자증권"        android:textColor="#8A8989"        android:textSize="14sp"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/tv_report_stock_title" />    <TextView        android:id="@+id/tv_report_stock_tag"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="16dp"        android:fontFamily="@font/noto_sans_cjk_kr_regular"        android:includeFontPadding="false"        android:letterSpacing="-0.07"        android:text="#하나투자증권 #인터넷"        android:textColor="#C53838"        android:textSize="14sp"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/tv_report_stock_name"       /></androidx.constraintlayout.widget.ConstraintLayout>